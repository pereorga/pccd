# Apache HTTP port.
APACHE_HTTP_PORT=8092

# Default base URL, used in scripts. Should match Apache's port. See also phpunit.xml.
BASE_URL=http://localhost:8092

# Password for the administration section (e.g. https://pccd.dites.cat/admin).
WEB_ADMIN_PASSWORD=contrasenyaweb

# MySQL credentials.
MYSQL_PASSWORD=contrasenyamysql
MYSQL_ROOT_PASSWORD=contrasenyarootmysql

# Optional: IP of the origin server. Used by Yarn check:certificate:origin script.
ORIGIN_IP=

# Optional: OpenAI Key for sentence tagging.
OPENAI_KEY=
